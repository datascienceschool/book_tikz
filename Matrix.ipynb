   "source": [
    "%%tikz -l graphs\n",
    "\n",
    "\\matrix[column sep=1cm, \n",
    "        every node/.style={minimum size=5mm}] {\n",
    "  \\node[draw] (a) {a};&  \n",
    "  \\node[draw] (X) {X};&\n",
    "  \\node[draw] (g) {g};\\\\\n",
    "};\n",
    "\n",
    "\\graph {\n",
    "    (a) -> (X) -> (g);\n",
    "    (a) ->[bend right] (g);\n",
    "};"
   ]
